mkdir run1
cd run1
wget http://psydata.ovgu.de/studyforrest/phase1/sub001/BOLD/task002_run005/bold_dico_bold7Tp1_to_subjbold7Tp1.nii.gz ### Downloading the data ###
gunzip bold_dico_bold7Tp1_to_subjbold7Tp1.nii.gz ### unziping it###
